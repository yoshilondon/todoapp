.container
  %h1.title All Boards
  - @boards.each do |board|
    .card
      .container_bracket
        %h2.title Brackets
        .card_content
          .card_title
            = board.title
        .dropdown-content-boards 
          %a{:href => "#"} Edit 
          %a{:href => "#"} Delete 
    .add_card 
      %p + Add new board

